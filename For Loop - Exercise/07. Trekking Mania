n_groups = int(input())

people_sum = 0

muss_people = 0
monblan_people = 0
kilim_people = 0
k2_people = 0
everest_people = 0

for i in range(1, n_groups+1):
    group = int(input())
    if group <= 5:
        muss_people += group
        people_sum += group
    elif 6 <= group <= 12:
        monblan_people += group
        people_sum += group
    elif 13 <= group <= 25:
        kilim_people += group
        people_sum += group
    elif 26 <= group <= 40:
        k2_people += group
        people_sum += group
    elif group >= 41:
        everest_people += group
        people_sum += group

muss_percent = muss_people / people_sum * 100
monblan_percent = monblan_people / people_sum * 100
kilim_percent = kilim_people / people_sum * 100
k2_percent = k2_people / people_sum * 100
everest_percent = everest_people / people_sum * 100

print(f"{muss_percent:.2f}%")
print(f"{monblan_percent:.2f}%")
print(f"{kilim_percent:.2f}%")
print(f"{k2_percent:.2f}%")
print(f"{everest_percent:.2f}%")
